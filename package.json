{
  "name": "n8n-nodes-katana-mrp",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Katana Cloud Manufacturing (Katana MRP) providing 12 resources and 70+ operations for manufacturing ERP automation, inventory management, production planning, and order fulfillment.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-katana-mrp.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-katana-mrp#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-katana-mrp/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "katana",
    "katana-mrp",
    "manufacturing",
    "erp",
    "inventory",
    "production",
    "mrp",
    "d2c",
    "automation",
    "workflow"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/KatanaMrpApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/KatanaMrp/KatanaMrp.node.js",
      "dist/nodes/KatanaMrp/KatanaMrpTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "copyfiles -u 1 \"nodes/**/*.svg\" \"nodes/**/*.png\" dist/",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.22.0",
    "prettier": "^3.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
